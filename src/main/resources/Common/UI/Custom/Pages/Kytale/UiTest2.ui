$C = "../../Common.ui";

$C.@PageOverlay {
  $C.@Container {
    Anchor: (Width: 550, Height: 650);

    #Title {
      Group {
        Label {
          Style: (FontSize: 24, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center);
          Text: "UI Test - Page 2";
        }
      }
    }

    #Content {
      LayoutMode: Top;
      Group #NavBar {
        Padding: (Horizontal: 16, Vertical: 12);
        LayoutMode: Left;
        $C.@TextButton #NavPage1 {
          Anchor: (Height: 36);
          FlexWeight: 1;
          Text: "1: Inputs";
          Style: TextButtonStyle(
            Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
            Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
            Sounds: $C.@ButtonSounds,
          );
        }
        $C.@TextButton #NavPage2 {
          Anchor: (Height: 36, Left: 8);
          FlexWeight: 1;
          Text: "2: Data";
          Style: TextButtonStyle(
            Default: (Background: #c4a23a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
            Hovered: (Background: #d4b24a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
            Sounds: $C.@ButtonSounds,
          );
        }
        $C.@TextButton #NavPage3 {
          Anchor: (Height: 36, Left: 8);
          FlexWeight: 1;
          Text: "3: Actions";
          Style: TextButtonStyle(
            Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
            Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
            Sounds: $C.@ButtonSounds,
          );
        }
      }
      Group #PageTitle {
        Padding: (Horizontal: 16, Vertical: 8);
        Label {
          Style: (FontSize: 24, TextColor: #ffffff, RenderBold: true);
          Text: "Data Management";
        }
      }
      Group #PaginationSection {
        Padding: (Horizontal: 16, Vertical: 12);
        LayoutMode: Top;
        Label {
          Style: (FontSize: 16, TextColor: #c4a23a, RenderBold: true);
          Text: "Pagination";
        }
        Group {
          Padding: (Top: 10);
          LayoutMode: Left;
          $C.@TextButton #FirstPage {
            Anchor: (Width: 40, Height: 32);
            Text: "<<";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #PrevPage {
            Anchor: (Width: 40, Height: 32, Left: 4);
            Text: "<";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #Page1 {
            Anchor: (Width: 36, Height: 32, Left: 8);
            Text: "1";
            Style: TextButtonStyle(
              Default: (Background: #c4a23a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #d4b24a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #Page2 {
            Anchor: (Width: 36, Height: 32, Left: 4);
            Text: "2";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #Page3 {
            Anchor: (Width: 36, Height: 32, Left: 4);
            Text: "3";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #Page4 {
            Anchor: (Width: 36, Height: 32, Left: 4);
            Text: "4";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #Page5 {
            Anchor: (Width: 36, Height: 32, Left: 4);
            Text: "5";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #NextPage {
            Anchor: (Width: 40, Height: 32, Left: 8);
            Text: ">";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #LastPage {
            Anchor: (Width: 40, Height: 32, Left: 4);
            Text: ">>";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
        }
      }
      Group #SortingSection {
        Padding: (Horizontal: 16, Vertical: 12);
        LayoutMode: Top;
        Label {
          Style: (FontSize: 16, TextColor: #c4a23a, RenderBold: true);
          Text: "Sorting";
        }
        Group {
          Padding: (Top: 10);
          LayoutMode: Left;
          Label {
            Anchor: (Width: 60);
            Style: (FontSize: 14, TextColor: #7c8b99, VerticalAlignment: Center);
            Text: "Sort by:";
          }
          $C.@TextButton #SortName {
            Anchor: (Width: 70, Height: 30);
            Text: "Name";
            Style: TextButtonStyle(
              Default: (Background: #c4a23a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #d4b24a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #SortDate {
            Anchor: (Width: 70, Height: 30, Left: 6);
            Text: "Date";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #SortSize {
            Anchor: (Width: 70, Height: 30, Left: 6);
            Text: "Size";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #SortType {
            Anchor: (Width: 70, Height: 30, Left: 6);
            Text: "Type";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
        }
        Group {
          Padding: (Top: 8);
          LayoutMode: Left;
          Label {
            Anchor: (Width: 60);
            Style: (FontSize: 14, TextColor: #7c8b99, VerticalAlignment: Center);
            Text: "Order:";
          }
          $C.@TextButton #SortAsc {
            Anchor: (Width: 100, Height: 30);
            Text: "Ascending";
            Style: TextButtonStyle(
              Default: (Background: #2d5a3d, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #3d7a4d, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #SortDesc {
            Anchor: (Width: 100, Height: 30, Left: 6);
            Text: "Descending";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
        }
      }
      Group #FilterSection {
        Padding: (Horizontal: 16, Vertical: 12);
        LayoutMode: Top;
        Label {
          Style: (FontSize: 16, TextColor: #c4a23a, RenderBold: true);
          Text: "Filters";
        }
        Group {
          Padding: (Top: 10);
          LayoutMode: Left;
          $C.@TextButton #FilterAll {
            Anchor: (Width: 60, Height: 30);
            Text: "All";
            Style: TextButtonStyle(
              Default: (Background: #c4a23a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #d4b24a, LabelStyle: (FontSize: 14, TextColor: #121a26, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #FilterActive {
            Anchor: (Width: 70, Height: 30, Left: 6);
            Text: "Active";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #FilterPending {
            Anchor: (Width: 80, Height: 30, Left: 6);
            Text: "Pending";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #FilterCompleted {
            Anchor: (Width: 60, Height: 30, Left: 6);
            Text: "Done";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
          $C.@TextButton #FilterArchived {
            Anchor: (Width: 80, Height: 30, Left: 6);
            Text: "Archived";
            Style: TextButtonStyle(
              Default: (Background: #1a2636, LabelStyle: (FontSize: 14, TextColor: #bfcdd5, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Hovered: (Background: #243448, LabelStyle: (FontSize: 14, TextColor: #ffffff, RenderBold: true, HorizontalAlignment: Center, VerticalAlignment: Center)),
              Sounds: $C.@ButtonSounds,
            );
          }
        }
      }
      Group #StatusSection {
        Padding: (Horizontal: 16, Vertical: 12);
        LayoutMode: Top;
        Label {
          Style: (FontSize: 16, TextColor: #c4a23a, RenderBold: true);
          Text: "Status";
        }
        Group {
          Padding: (Top: 10);
          LayoutMode: Left;
          Group {
            Anchor: (Width: 140);
            LayoutMode: Top;
            Label {
              Style: (FontSize: 12, TextColor: #7c8b99);
              Text: "Total Items";
            }
            Label #TotalItems {
              Style: (FontSize: 32, TextColor: #ffffff, RenderBold: true);
              Text: "1,247";
            }
          }
          Group {
            Anchor: (Width: 140, Left: 20);
            LayoutMode: Top;
            Label {
              Style: (FontSize: 12, TextColor: #7c8b99);
              Text: "Showing";
            }
            Label #ShowingRange {
              Style: (FontSize: 32, TextColor: #c4a23a, RenderBold: true);
              Text: "1 - 25";
            }
          }
          Group {
            Anchor: (Width: 140, Left: 20);
            LayoutMode: Top;
            Label {
              Style: (FontSize: 12, TextColor: #7c8b99);
              Text: "Selected";
            }
            Label #SelectedCount {
              Style: (FontSize: 32, TextColor: #4ecdc4, RenderBold: true);
              Text: "3";
            }
          }
        }
      }
    }
  }
}

$C.@BackButton {}
